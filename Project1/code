void setup() {
  pinMode (4, OUTPUT);
  pinMode (23, OUTPUT);
  pinMode (2, OUTPUT);
  pinMode(15, INPUT);
  Serial.begin(9600);
}
void loop() {
  int speed=34.3;
  digitalWrite(2, HIGH);
  delay(10);
  digitalWrite(2,LOW);
  long duration=pulseIn(15,HIGH);
  float distance=duration*speed/2000;
  Serial.println(distance);
  if (distance<=100){
    digitalWrite(4, HIGH);
    digitalWrite(23, LOW);
  }
  else{
    digitalWrite(23, HIGH);
    digitalWrite(4, LOW);
  }
}
